{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File", "Serilog.Sinks.Graylog" ],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Properties": {
      "Application": "SnWebApplication.Api.InMem.TokenAuth",
      "Repository": ""
    }
  },
  "AllowedHosts": "*",
  "sensenet": {
    "authentication": {
      "authority": "https://localhost:44311",
      "metadatahost": "http://SenseNet.IdentityServer4.Web",
      "repositoryUrl": "https://localhost:44362",
      "AddJwtCookie": true
    }
  }
}
