{
  "ConnectionStrings": {
    "SnCrMsSql": ""
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File", "Serilog.Sinks.Graylog" ],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Properties": {
      "Application": "SnWebApplication.Api.Sql.SearchService.TokenAuth",
      "Repository": ""
    }
  },
  "AllowedHosts": "*",
  "sensenet": {
    "authentication": {
      "authServerType": "IdentityServer",
      "authority": "https://localhost:44311",
      "repositoryUrl": "https://localhost:44362",
      "AddJwtCookie": true,
      "TokenValidatorKey": "test-token"
    },
    "Email": {
      "Server": "",
      "Port": 587
    },
    "Registration": {
      "Groups": [],
      "UserType": ""
    },
    "search": {
      "service": {
        "ServiceAddress": "https://localhost:44322",
        "ValidateServerCertificate": false,
        "ServiceWritePartitionSize": 30
      }
    },
    "rabbitmq": {
      "ServiceUrl": ""
    },
    "security": {
      "rabbitmq": {
        "ServiceUrl": ""
      }
    },
    "Retrier": {
      "Count": 30,
      "WaitMilliseconds": 2000
    }
  }
}
