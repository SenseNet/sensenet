{
  "ConnectionStrings": {
    "SnCrMsSql": ""
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File", "Serilog.Sinks.Graylog" ],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Properties": {
      "Application": "SnWebApplication.Api.Sql.TokenAuth",
      "Repository": ""
    }
  },
  "AllowedHosts": "*",
  "sensenet": {
    "authentication": {
      "authServerType": "SNAuth",
      "authority": "https://localhost:7088",
      "repositoryUrl": "https://localhost:44362",
      "AddJwtCookie": false
    },
    "Email": {
      "Server": "",
      "Port": 587
    },
    "Registration": {
      "Groups": [],
      "UserType": ""
    }
  }
}
