{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File", "Serilog.Sinks.Graylog" ],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Properties": {
      "Application": "SnWebApplication.Api.InMem.Admin",
      "Repository": ""
    }
  },
  "AllowedHosts": "*",
  "sensenet": {
    "Email": {
      "Server": "",
      "Port": 587
    },
    "Authentication": {
      "AddJwtCookie": true
    }
  }
}
