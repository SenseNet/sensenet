  </metadata>
  <files>
    <file src="..\nuget\readme.txt" target="" />
    <file src="..\nuget\content\**" target="content" />
    <file src="..\Tools\SnAdminRuntime\bin\Release\SnAdminRuntime.*" target="content\Tools" />
