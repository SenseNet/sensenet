        <dependency id="SenseNet.Security.EF6SecurityStore" version="4.1.0" />
        <dependency id="SenseNet.SnAdmin" version="1.4.4" />
        <dependency id="SenseNet.Search.Lucene29.Local" version="7.4.2" />
      </group>
