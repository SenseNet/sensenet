<?xml version="1.0" encoding="utf-8"?>
<Resources>
  <ResourceClass name="Ctd-Resource">
    <Languages>
      <Language cultureName="en">
        <data name="DisplayName" xml:space="preserve">
          <value>Resource</value>
        </data>
        <data name="Description" xml:space="preserve">
          <value>String or binary resource used to localize the system. Its format is the same as the internal part of a standard .Net resx file.</value>
        </data>
        <data name="Binary-DisplayName" xml:space="preserve">
          <value>Binary</value>
        </data>
        <data name="Downloads-DisplayName" xml:space="preserve">
          <value>Downloads</value>
        </data>
        <data name="Downloads-Description" xml:space="preserve">
          <value>The number of downloads.</value>
        </data>
        <data name="Size-DisplayName" xml:space="preserve">
          <value>Size</value>
        </data>
        <data name="Size-Description" xml:space="preserve">
          <value>Size of this version.</value>
        </data>
        <data name="FullSize-DisplayName" xml:space="preserve">
          <value>Full size</value>
        </data>
        <data name="FullSize-Description" xml:space="preserve">
          <value>Size of all versions together.</value>
        </data>
      </Language>
      <Language cultureName="hu">
        <data name="DisplayName" xml:space="preserve">
          <value>Erőforrás fájl</value>
        </data>
        <data name="Description" xml:space="preserve">
          <value>Szöveges vagy bináris fájl a rendszer lokalizálására. Formátuma megegyezik a resx fájlokéval.</value>
        </data>
        <data name="Binary-DisplayName" xml:space="preserve">
          <value>Bináris</value>
        </data>
        <data name="Binary-Description" xml:space="preserve">
          <value>A dokumentum binárisa.</value>
        </data>
        <data name="Size-DisplayName" xml:space="preserve">
          <value>Méret</value>
        </data>
        <data name="Size-Description" xml:space="preserve">
          <value>Bináris dokumentum mérete.</value>
        </data>
        <data name="FullSize-DisplayName" xml:space="preserve">
          <value>Teljes méret</value>
        </data>
        <data name="FullSize-Description" xml:space="preserve">
          <value>A dokumentum teljes mérete, amely tartalmazza a dokumentum összes korábbi verzióját is.</value>
        </data>
        <data name="Downloads-DisplayName" xml:space="preserve">
          <value>Letöltések</value>
        </data>
        <data name="Downloads-Description" xml:space="preserve">
          <value>Letöltések száma.</value>
        </data>
      </Language>
    </Languages>
  </ResourceClass>
