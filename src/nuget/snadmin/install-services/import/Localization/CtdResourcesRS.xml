  <ResourceClass name="Ctd-Settings">
    <Languages>
      <Language cultureName="en">
        <data name="DisplayName" xml:space="preserve">
          <value>Settings</value>
        </data>
        <data name="Description" xml:space="preserve">
          <value>Content type for storing application or module settings in text format or in custom fields.</value>
        </data>
        <data name="GlobalOnly-DisplayName" xml:space="preserve">
          <value>Global only</value>
        </data>
        <data name="GlobalOnly-Description" xml:space="preserve">
          <value>Switching this ON will prevent the creation of local settings with the same name preventing others to gain access to the contents of this settings file through inheritance.</value>
        </data>
      </Language>
      <Language cultureName="hu">
        <data name="DisplayName" xml:space="preserve">
          <value>Beállítás</value>
        </data>
        <data name="Description" xml:space="preserve">
          <value>Beállítások tárolására szolgáló tartalom típus.</value>
        </data>
      <data name="GlobalOnly-DisplayName" xml:space="preserve">
          <value>Globális beállítások</value>
        </data>
        <data name="GlobalOnly-Description" xml:space="preserve">
          <value>Ha ezt bekapcsolja, senki nem hozhat létre helyi beállításokat ugyanezzel a névvel. Ez megakadályozza, hogy a beállítás-öröklés miatt más is hozzáférjen a globális beállításokhoz.</value>
        </data>
      </Language>
    </Languages>
  </ResourceClass> 
  <ResourceClass name="Ctd-SmartFolder">
    <Languages>
      <Language cultureName="en">
        <data name="DisplayName" xml:space="preserve">
          <value>Smart Folder</value>
        </data>
        <data name="Description" xml:space="preserve">
          <value>Use smart folders to group information (content) by Repository query.</value>
        </data>
        <data name="Query-DisplayName" xml:space="preserve">
          <value>Query</value>
        </data>
        <data name="Query-Description" xml:space="preserve">
          <value>Please give a query here that you want to use for collecting the children of this smart folder.</value>
        </data>
        <data name="EnableAutofilters-DisplayName" xml:space="preserve">
          <value>Enable autofilters</value>
        </data>
        <data name="EnableAutofilters-Description" xml:space="preserve">
          <value>If autofilters are enabled, system content will be filtered from the query.</value>
        </data>
        <data name="EnableLifespanFilter-DisplayName" xml:space="preserve">
          <value>Enable lifespan filter</value>
        </data>
        <data name="EnableLifespanFilter-Description" xml:space="preserve">
          <value>If lifespan filter is enabled, only valid content will be in the result.</value>
        </data>
      </Language>
      <Language cultureName="hu">
        <data name="DisplayName" xml:space="preserve">
          <value>Okosmappa</value>
        </data>
        <data name="Description" xml:space="preserve">
          <value>Különböző szűrési feltételek alapján kigyűjtött tartalmak tárolására.</value>
        </data>
        <data name="Query-DisplayName" xml:space="preserve">
          <value>Query</value>
        </data>
        <data name="Query-Description" xml:space="preserve">
          <value>Adja meg azokat a feltételeket, amelyek alapján a mappába szeretne gyűjteni tartalmakat.</value>
        </data>
        <data name="EnableAutofilters-DisplayName" xml:space="preserve">
          <value>Automata szűrési feltételek bekapcsolása</value>
        </data>
        <data name="EnableAutofilters-Description" xml:space="preserve">
          <value>Ha be van kapcsolva, a rendszer fájlok nem kerülnek bele a mappába.</value>
        </data>
        <data name="EnableLifespanFilter-DisplayName" xml:space="preserve">
          <value>Élettartam szűrés bekapcsolása</value>
        </data>
        <data name="EnableLifespanFilter-Description" xml:space="preserve">
          <value>Ha be van kapcsolva, akkor csak az időben aktuális tartalmak kerülnek be.</value>
        </data>
      </Language>
    </Languages>
  </ResourceClass>
