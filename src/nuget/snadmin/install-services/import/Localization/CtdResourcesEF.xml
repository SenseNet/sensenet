<?xml version="1.0" encoding="utf-8"?>
<?xml-stylesheet type="text/xsl" href="view.xslt"?>
<Resources>
  <ResourceClass name="Ctd-Email">
    <Languages>
      <Language cultureName="en">
        <data name="DisplayName" xml:space="preserve">
          <value>Email</value>
        </data>
        <data name="Description" xml:space="preserve">
          <value>Email content type containing attachments as children content.</value>
        </data>
        <data name="From-DisplayName" xml:space="preserve">
          <value>From</value>
        </data>
        <data name="From-Description" xml:space="preserve">
          <value>Sender name and address.</value>
        </data>
        <data name="DisplayName-DisplayName" xml:space="preserve">
          <value>Subject</value>
        </data>
        <data name="DisplayName-Description" xml:space="preserve">
          <value>Subject of email.</value>
        </data>
        <data name="Body-DisplayName" xml:space="preserve">
          <value>Body</value>
        </data>
        <data name="Body-Description" xml:space="preserve">
          <value>Body of email.</value>
        </data>
        <data name="Sent-DisplayName" xml:space="preserve">
          <value>Sent</value>
        </data>
        <data name="Sent-Description" xml:space="preserve">
          <value>Date of sending.</value>
        </data>
      </Language>
      <Language cultureName="hu">
        <data name="DisplayName" xml:space="preserve">
          <value>Email</value>
        </data>
        <data name="Description" xml:space="preserve">
          <value>Email tartalomtípus, melynek gyermekelemei lehetnek a csatolmányai.</value>
        </data>
        <data name="From-DisplayName" xml:space="preserve">
          <value>Feladó</value>
        </data>
        <data name="From-Description" xml:space="preserve">
          <value>Feladó neve és címe.</value>
        </data>
        <data name="DisplayName-DisplayName" xml:space="preserve">
          <value>Tárgy</value>
        </data>
        <data name="DisplayName-Description" xml:space="preserve">
          <value>Az email tárgya.</value>
        </data>
        <data name="Body-DisplayName" xml:space="preserve">
          <value>Törzsszöveg</value>
        </data>
        <data name="Body-Description" xml:space="preserve">
          <value>Az email törzsszövege.</value>
        </data>
        <data name="Sent-DisplayName" xml:space="preserve">
          <value>Elküldve</value>
        </data>
        <data name="Sent-Description" xml:space="preserve">
          <value>Küldés dátuma.</value>
        </data>
      </Language>
    </Languages>
  </ResourceClass>
  <ResourceClass name="Ctd-ExecutableFile">
    <Languages>
      <Language cultureName="en">
        <data name="DisplayName" xml:space="preserve">
          <value>Executable File</value>
        </data>
        <data name="Description" xml:space="preserve">
          <value>Only content of this type can be executed directly (e.g. aspx files).</value>
        </data>
      </Language>
      <Language cultureName="hu">
        <data name="DisplayName" xml:space="preserve">
          <value>Futtatható fájl</value>
        </data>
        <data name="Description" xml:space="preserve">
          <value>Csak ilyen típusú tartalmak futtathatók közvetlenül (pl. aspx fájlok).</value>
        </data>
      </Language>
    </Languages>
  </ResourceClass>
  <ResourceClass name="Ctd-ExportToCsvApplication">
    <Languages>
      <Language cultureName="en">
        <data name="DisplayName" xml:space="preserve">
          <value>Export To CSV application</value>
        </data>
        <data name="Description" xml:space="preserve">
          <value>Generates CSV file from a content list.</value>
        </data>
      </Language>
      <Language cultureName="hu">
        <data name="DisplayName" xml:space="preserve">
          <value>Exportálás CSV-be</value>
        </data>
        <data name="Description" xml:space="preserve">
          <value>Alkalmazás, amely listából letölthető CSV-fájlt generál.</value>
        </data>
      </Language>
    </Languages>
  </ResourceClass>
  <ResourceClass name="Ctd-FieldSettingContent">
    <Languages>
      <Language cultureName="en">
        <data name="DisplayName" xml:space="preserve">
          <value>Field Setting</value>
        </data>
        <data name="Description" xml:space="preserve">
          <value>System type to handle field editing in ContentLists.</value>
        </data>
        <data name="Name-Description" xml:space="preserve">
          <value>Specify a name for the field that you will use in content views and queries.  
            <div class='ui-helper-clearfix sn-dialog-editportlet-warning'><img class='sn-icon sn-icon16 sn-floatleft' src='/Root/Global/images/icons/16/warning.png' />
              <i>Please do not use any special characters, for example comma, dash ('-') or brackets ( '(' )</i>
            </div>
          </value>
        </data>
      </Language>
      <Language cultureName="hu">
        <data name="DisplayName" xml:space="preserve">
          <value>Mező leíró</value>
        </data>
        <data name="Description" xml:space="preserve">
          <value>Csak a rendszer által használható típus lista mezők szerkesztésére.</value>
        </data>
        <data name="Name-Description" xml:space="preserve">
          <value>Adjon meg egy nevet a mezőnek, amelyet megjelenítőkben és lekérdezésekben szeretne használni.  
            <div class='ui-helper-clearfix sn-dialog-editportlet-warning'><img class='sn-icon sn-icon16 sn-floatleft' src='/Root/Global/images/icons/16/warning.png' />
              <i>Kérem, ne használjon speciális karaktereket, például szóköz, vessző, kötőjel ('-') vagy zárójel.</i>
            </div>
          </value>
        </data>
      </Language>
    </Languages>
  </ResourceClass>
  <ResourceClass name="Ctd-File">
    <Languages>
      <Language cultureName="en">
        <data name="DisplayName" xml:space="preserve">
          <value>File</value>
        </data>
        <data name="Description" xml:space="preserve">
          <value>A type for binary documents, images, etc.</value>
        </data>
        <data name="Binary-DisplayName" xml:space="preserve">
          <value>Binary</value>
        </data>
        <data name="Binary-Description" xml:space="preserve">
          <value>The binary content of the document.</value>
        </data>
        <data name="Size-DisplayName" xml:space="preserve">
          <value>Size</value>
        </data>
        <data name="Size-Description" xml:space="preserve">
          <value>Size of the binary document.</value>
        </data>
        <data name="FullSize-DisplayName" xml:space="preserve">
          <value>Full size</value>
        </data>
        <data name="FullSize-Description" xml:space="preserve">
          <value>The total amount of space the Document occupies, counting all versions.</value>
        </data>
        <data name="MimeType-DisplayName" xml:space="preserve">
          <value>Document MIME type</value>
        </data>
        <data name="PageCount-DisplayName" xml:space="preserve">
          <value>Page count</value>
        </data>
        <data name="PageCount-Description" xml:space="preserve">
          <value>Read-only field for storing the number of pages in the document. It is filled by the document preview generator.</value>
        </data>
        <data name="Shapes-DisplayName" xml:space="preserve">
          <value>Shapes</value>
        </data>
        <data name="Shapes-Description" xml:space="preserve">
          <value>Stores data used for document preview (redaction, highlight, annotation shapes). This value can be modified by the document preview plugin.</value>
        </data>
        <data name="PageAttributes-DisplayName" xml:space="preserve">
          <value>Page attributes</value>
        </data>
        <data name="PageAttributes-Description" xml:space="preserve">
          <value>Stores data used for document preview (for example page rotation). This value can be modified by the document preview plugin.</value>
        </data>
        <data name="Watermark-DisplayName" xml:space="preserve">
          <value>Watermark</value>
        </data>
        <data name="Watermark-Description" xml:space="preserve">
          <value>The text that is displayed as a watermark on the document preview. The format can be set by modifying the Document Preview settings.</value>
        </data>
      </Language>
      <Language cultureName="hu">
        <data name="DisplayName" xml:space="preserve">
          <value>Fájl</value>
        </data>
        <data name="Description" xml:space="preserve">
          <value>Dokumentumok, képek és egyéb szöveges vagy bináris tartalmak tárolására szolgáló típus.</value>
        </data>
        <data name="Binary-DisplayName" xml:space="preserve">
          <value>Bináris</value>
        </data>
        <data name="Binary-Description" xml:space="preserve">
          <value>A dokumentum binárisa.</value>
        </data>
        <data name="Size-DisplayName" xml:space="preserve">
          <value>Méret</value>
        </data>
        <data name="Size-Description" xml:space="preserve">
          <value>Bináris dokumentum mérete.</value>
        </data>
        <data name="FullSize-DisplayName" xml:space="preserve">
          <value>Teljes méret</value>
        </data>
        <data name="FullSize-Description" xml:space="preserve">
          <value>A dokumentum teljes mérete, amely tartalmazza a dokumentum összes korábbi verzióját is.</value>
        </data>
        <data name="MimeType-DisplayName" xml:space="preserve">
          <value>A dokumentum MIME type-ja (például css, jpeg, egyszerű szöveg)</value>
        </data>
        <data name="PageCount-DisplayName" xml:space="preserve">
          <value>Oldalak száma</value>
        </data>
        <data name="PageCount-Description" xml:space="preserve">
          <value>Csak olvasható mező a dokumentumban található oldalak számának tárolására. Az előnézet generáló alkalmazás tölti ki.</value>
        </data>
        <data name="Shapes-DisplayName" xml:space="preserve">
          <value>Előnézeti kiegészítő formák</value>
        </data>
        <data name="Shapes-Description" xml:space="preserve">
          <value>A kiemelés, titkosítás, megjegyzés dobozok adatait tartalmazó mező.</value>
        </data>
        <data name="PageAttributes-DisplayName" xml:space="preserve">
          <value>Előnézeti kép adatok</value>
        </data>
        <data name="PageAttributes-Description" xml:space="preserve">
          <value>Előnézeti képek adatait (például az elforgatás mértékét) tartalmazó mező.</value>
        </data>
        <data name="Watermark-DisplayName" xml:space="preserve">
          <value>Vízjel</value>
        </data>
        <data name="Watermark-Description" xml:space="preserve">
          <value>Vízjel szöveg, amely az előnézeti képeken látható. A formátum az Előnézet beállításoknál módosítható.</value>
        </data>
      </Language>
    </Languages>
  </ResourceClass>
  <ResourceClass name="Ctd-Folder">
    <Languages>
      <Language cultureName="en">
        <data name="DisplayName" xml:space="preserve">
          <value>Folder</value>
        </data>
        <data name="Description" xml:space="preserve">
          <value>Use folders to group content.</value>
        </data>
        <data name="DisplayName-DisplayName" xml:space="preserve">
          <value>Folder name</value>
        </data>
        <data name="DisplayName-Description" xml:space="preserve">
          <value>Display name of the content. You can set any name you prefer without any restrictions.</value>
        </data>
      </Language>
      <Language cultureName="hu">
        <data name="DisplayName" xml:space="preserve">
          <value>Mappa</value>
        </data>
        <data name="Description" xml:space="preserve">
          <value>Mappa tartalmak csoportosítására.</value>
        </data>
        <data name="DisplayName-DisplayName" xml:space="preserve">
          <value>Mappa neve</value>
        </data>
        <data name="DisplayName-Description" xml:space="preserve">
          <value></value>
        </data>
      </Language>
    </Languages>
  </ResourceClass>
</Resources>
