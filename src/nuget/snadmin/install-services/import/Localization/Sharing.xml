          <value>Shared content</value>
        </data>
        <data name="NotificationMailBody" xml:space="preserve">
          <value><p><h2>Shared content</h2><div>Click on the following link to open it.</div><div>{0}</div></p><p><div>Sharing level: <strong>{1}</strong></div></p></value>
